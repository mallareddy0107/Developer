           <apex:outputText id="col1" value="{!firstdt}">
    
          </apex:outputText>
          <apex:outputText id="col2" value="{!seconddt}">
          </apex:outputText>
          
          <apex:outputText value="{!thirddt}">
          </apex:outputText>
          
          <apex:outputText value="{!fourthdt}">
          </apex:outputText>
          
          <apex:outputText value="{!fifthdt}">
          </apex:outputText>
          
          <apex:outputText value="{!sixthdt}">
          </apex:outputText>
          
          <apex:outputText value="{!seventhdt}">
          </apex:outputText>
    
      </apex:panelGrid>
  
   </center>
   </apex:pageblocksection>    
 </apex:pageBlock>
</apex:form>
</center>
</apex:page>